<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mood">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_panda"
        android:usesCleartextTraffic="true"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_panda"
        android:supportsRtl="true"
        android:theme="@style/Theme.Mood">
        <activity android:name=".MusicList"></activity>
        <activity android:name=".EmojiPage" />
        <activity android:name=".PlayerActivity"/>
        <activity android:name=".Chatbot"></activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".Module">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


    </application>

</manifest>